
<view class="setting_head" style="background-image: url('../../images/my/xu.png')">
	<view class="head_pic">
		  <image src="../../images/my/touper.png"></image>

		<view>
			<text class="setting_username2">{{ yourname }}</text>
			<text class="setting_username">|</text>
			<text class="setting_username2">mmm</text>
		</view>
	</view>
</view>



<view class="listContainer">
	<view class="someList">
		<view class="box">
			<image src="../../images/my/person.png" class="iconOne"></image>
			<text>我的信息</text>
			<image src="../../images/my/next.png" class="iconTwo"></image>
		</view>
	</view>
	<view class="someList2">
		<view class="box">
			<image src="../../images/my/history.png" class="iconOne"></image>
			<text>我的历史</text>
			<image src="../../images/my/next.png" class="iconTwo"></image>
		</view>
		<view class="box" bindtap="goGeneralSetting">
			<image src="../../images/my/she.png" class="iconOne"></image>
			<text>通用设置</text>
			<image src="../../images/my/next.png" class="iconTwo"></image>
		</view>
		<view class="box" bindtap="tab_notification">
			<image src="../../images/my/backnew.png" class="iconOne"></image>
			<text>返回之前</text>
			<image src="../../images/my/next.png" class="iconTwo"></image>
		</view>
	</view>
</view>

<view class="someList">
	<view class="box toBot" bindtap="logout">
		<image src="../../images/my/exit.png" class="iconOne"></image>
		<text>退出登录</text>
		<image src="../../images/my/next.png" class="iconTwo"></image>
	</view>
</view>

