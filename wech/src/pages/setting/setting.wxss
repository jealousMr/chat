page {
	background-color: #F5F5F5;
}
.setting_head {
	width: 100%;
	height: 460rpx;
}
.head_pic {
	height: 100%;
	display: block;
	align-items: center;
	padding-top: 80rpx;
}
.head_pic view{
	margin-top: 50rpx;
	margin-left: 34%;
	display: flex;
}
.head_pic image {
	margin-left: 36%;
	width: 200rpx;
	height: 200rpx;
	border-radius: 100rpx;
	display: block;
	border:1px solid rgba(227, 227, 230, 0.71);
}

.setting_username {
	height: 68rpx;
	line-height: 68rpx;
	font-size: 40rpx;
	display: block;
	color: #d6d6d6;
	margin-left: 20rpx;
	margin-right: 20rpx;
}

.setting_username2{
	height: 54rpx;
	line-height: 54rpx;
	font-size: 34rpx;
	color: #ebebeb;
	display: block;
	margin-top: 20rpx;
}


.setting_list{
	background-color: #fff;
}
.setting_listContent {
	height: 88rpx;
	margin: 48rpx 32rpx 0;
	border-bottom: 0.5px #E5E5E5 solid;
	display: flex;
	justify-content: space-between;
	align-items: center;
	font-size: 32rpx;
	background-color: #fff;
}
.list_right {
	width: 48rpx;
	height: 48rpx;
	margin-right: 8rpx;
	color: #9B9B9B;
}



.listContainer{
	padding-top: 16rpx;
	background-color: #eeeeee;
}

.someList{
	background-color: #ffffff;
	display: block;
	padding: 0px;
}
.box{
	display: flex;
	border-bottom:0.6px solid #cfcfd2;
	width: 100%;
	height: 100rpx;
	padding: 5rpx;
}
.box image{
	width: 60rpx;
	height: 60rpx;
}
.box .iconOne{
	margin-left: 2%;
	margin-top: 3%;
}
.box .iconTwo{
	margin-top: 2%;
	margin-left: 58%;
}
.box text{
	margin-left: 50rpx;
	margin-top: 4%;
	font-size: 30rpx;
}
.someList2{
	margin-top: 50rpx;
	background-color: #ffffff;
	display: block;
	padding: 0px;
}
.toBot{
	margin-top: 40rpx;
}
